{
    "name": "trollandtoad/sellbrite-sdk",
    "description": "A PHP library that interacts with the Sellbrite API.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "Sellbrite",
        "Sellbrite SDK",
        "PHP",
        "PHP SDK"
    ],
    "authors": [
        {
            "name": "Samuel Stidham",
            "email": "sastidham@trollandtoad.com"
        },
        {
            "name": "Brandon Clothier",
            "email": "brclothier@trollandtoad.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "TrollAndToad\\Sellbrite\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TrollAndToad\\Sellbrite\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.3",
        "carbondate/carbon": "^1.33"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "mockery/mockery": "^1.2"
    },
    "config": {
        "sort-packages": true
    }
}
